{
  "expo": {
    "name": "Waiter App",
    "slug": "waiter-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "backgroundColor": "#d73035",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#d73035"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mateuscorreiaazevedo.waiterapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#d73035"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.mateuscorreiaazevedo.waiterapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#d73035",
          "image": "./assets/icon.png",
          "dark": {
            "backgroundColor": "#242424"
          },
          "imageWidth": 200
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7f3d758e-7fe9-4290-a050-54536e041270"
      }
    },
    "owner": "mateuscorreiaazevedo"
  }
}
